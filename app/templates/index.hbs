
  <div class="wrapper">
    <div class="page-header">
      <img src="/image/logo.png">
    </div>
    <div class='container body'>
      <div class="row">
        <div class="questions">
          <h1>Question</h1>
          <ol>

            {{#each model.questions as |question|}}
              <div class="eachQuiz">
                {{question-tile question=question}}
              </div>
            {{/each}}

          </ol>
        </div>
        <div class="col-md-6 col-sm-6">
          {{new-question saveQuestion="saveQuestion"}}
        </div>
      </div>
    </div>
  </div>
